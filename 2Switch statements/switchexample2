let day=new Date().getDay
switch(day){
        case'monday':
        console.log("hey its on a monday");
        break;
    case 'Tuesday':
        console.log("leo ni tuesday");
        break;
    case 'wednesday':
        console.log("Leo ni wednesday");
        break;
    case 'Thursday':
        console.log("Leo ni TBT");
        break;
    case 'Friday':
        console.log("Furahi Day");
    case 'Saturday':
        console.log("leo ni sato");
        break;
    case 'Sunday':
        break;
    default:
        console.log("Invalid Day");
}